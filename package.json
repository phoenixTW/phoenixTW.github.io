    "semantic-ui-react": "^0.88.1"
  },
  "keywords": [
    "phoenixtw.github.io",
    "persona-website",
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
